{
  "name": "ocimatic-testplan",
  "displayName": "ocimatic-testplan",
  "description": "",
  "version": "0.0.3",
  "repository": "https://github.com/OCIoficial/ocimatic",
  "publisher": "nlehmann",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ocimatic-testplan",
        "aliases": [
          "ocimatic-testplan"
        ],
        "filenamePatterns": [
          "testplan.txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ocimatic-testplan",
        "scopeName": "source.ocimatic-testplan",
        "path": "./syntaxes/ocimatic-testplan.tmLanguage.json"
      }
    ]
  },
  "main": "./client/out/extension",
  "activationEvents": [],
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "eslint": "^9.13.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vsce": "^2.15.0"
  },
  "scripts": {
    "vscode:prepublish": "cd client && npm install && cd .. && npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "lint": "eslint",
    "postinstall": "cd client && npm install",
    "package": "vsce package"
  }
}
